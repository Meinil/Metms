<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="550.0" prefWidth="900.0" spacing="10.0" stylesheets="@../../css/Base.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.meinil.metms.client.controller.teacher.MyStudentController">
   <children>
      <TableView fx:id="tcStudent" prefHeight="513.0" prefWidth="860.0">
        <columns>
          <TableColumn fx:id="tcName" prefWidth="75.0" text="姓名" />
          <TableColumn fx:id="tcAccount" prefWidth="75.0" text="学号" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
