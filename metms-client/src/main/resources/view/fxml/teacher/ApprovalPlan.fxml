<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="550.0" prefWidth="900.0" stylesheets="@../../css/Base.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.meinil.metms.client.controller.teacher.ApprovalPlanController">
	<padding>
		<Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
	</padding>
   <children>
      <TableView fx:id="tcPlan" prefHeight="512.0" prefWidth="860.0">
        <columns>
          <TableColumn fx:id="tcTitle" prefWidth="75.0" text="导师课名称" />
          <TableColumn fx:id="tcStudent" prefWidth="75.0" text="学生" />
            <TableColumn fx:id="tcCreateTime" prefWidth="75.0" text="提交日期" />
            <TableColumn fx:id="tcLevel" prefWidth="75.0" text="等级" />
            <TableColumn fx:id="tcApprove" prefWidth="75.0" text="批改" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</VBox>
